<html>
	<head>
		{{template "head" .}}
	</head>
	<body>
		{{.BodyPre}}
		{{.BodyTop}}
		{{template "notifications" .RepoNotifications}}
	</body>
</html>

{{define "notifications"}}
{{with .}}{{range .}}
	{{render .}}
{{end}}{{else}}
	<div style="text-align: center; margin-top: 80px; margin-bottom: 80px;">No new notifications.</div>
{{end}}
{{end}}
